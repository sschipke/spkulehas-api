{
  "name": "spkulehas-api",
  "version": "1.0.0",
  "description": "Api for managing reservations for the SpKuLeHaS Cabin",
  "license": "UNLICENSED",
  "author": "Scott Schipke",
  "main": "index.js",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn format:check",
      "pre-push": "yarn lint && yarn format:check"
    }
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --exec babel-node src/server.js --verbose",
    "compile": "npx babel src --out-dir build --copy-files && cp package.json ./build && cp yarn.lock ./build && cp -r ./config ./build",
    "initdb": "knex migrate:latest && knex seed:run",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "addNewSeasonReservations": "knex seed:run --specific=add_new_reservations.js",
    "down": "knex migrate:down",
    "test": "jest --forceExit",
    "test:watch": "jest --watchAll",
    "lint": "eslint .  --ext .js",
    "lint:fix": "eslint --fix .  --ext .js",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "prepare": "husky install",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "test:build": "yarn compile && cp .env ./build && cd ./build && node server.js"
  },
  "dependencies": {
    "@babel/core": "^7.25.9",
    "@babel/node": "^7.13.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "bcrypt": "^5.0.1",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "express": "^4.17.1",
    "express-rate-limit": "^6.4.0",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.4.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.7.3",
    "nodemailer-express-handlebars": "^5.0.0",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.5.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "babel-jest": "^24.9.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^8.0.3",
    "pinst": "^3.0.0",
    "prettier": "^3.0.3",
    "regenerator-runtime": "^0.13.7",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "yarn": ">=1.22.22"
  }
}
